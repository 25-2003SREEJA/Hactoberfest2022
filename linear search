#include<stdio.h>
#include<conio.h>
void main()
{
int n,a[100],ele,t=0;
printf("enter size of array:");
scanf("%d",&n);
printf("enter elements of array:");
for(int i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("enter the element you want to search:");
scanf("%d",&ele);
for(int i=0;i<n;i++)
{
if(ele==a[i])
{
t=i;
break;
}
}
if(t!=0)
 printf("Index of searched element is %d",t);
else
 printf("element is not found");
}
 
